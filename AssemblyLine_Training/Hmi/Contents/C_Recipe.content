<div data-tchmi-grid-row-index="0" data-tchmi-grid-column-index="0" id="gtnDownload" data-tchmi-type="TcHmi.Controls.Beckhoff.TcHmiButton" data-tchmi-height="50" data-tchmi-height-unit="px" data-tchmi-left="544" data-tchmi-left-unit="px" data-tchmi-text="Download" data-tchmi-top="23" data-tchmi-top-unit="px" data-tchmi-width="100" data-tchmi-width-unit="px" data-tchmi-word-wrap="true">

<script data-tchmi-target-attribute="data-tchmi-trigger" type="application/json">
[
  {
    "event": "gtnDownload.onPressed",
    "actions": [
      {
        "objectType": "Function",
        "active": true,
        "fn": "DownloadRecipe",
        "fnParams": [
          {
            "objectType": "Symbol",
            "valueType": "tchmi:framework#/definitions/RecipeReference",
            "symbolExpression": "%ctrl%rsMain::SelectedRecipeFullName%/ctrl%"
