</div>
<div id="gtnUpload" data-tchmi-type="TcHmi.Controls.Beckhoff.TcHmiButton" data-tchmi-height="50" data-tchmi-height-unit="px" data-tchmi-left="408" data-tchmi-left-unit="px" data-tchmi-text="Upload" data-tchmi-top="23" data-tchmi-top-unit="px" data-tchmi-width="100" data-tchmi-width-unit="px" data-tchmi-word-wrap="true">

<script data-tchmi-target-attribute="data-tchmi-trigger" type="application/json">
[
  {
    "event": "gtnUpload.onPressed",
    "actions": [
      {
        "objectType": "Function",
        "active": true,
        "fn": "UploadRecipe",
        "fnParams": [],
        "asyncWait": true
      }
    ]
  }
]
</script>
